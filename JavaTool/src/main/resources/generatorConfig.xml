<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <context id="context1">
        <!-- 是否生成注释 -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!-- 数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL=""
                        userId="" password="" />
        <!-- 生成的包名和工程名 -->
        <javaModelGenerator targetPackage="tca.gatewayManage.model.unifiedService"
                            targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="tca.gatewayManage.mapper.unifiedService"
                         targetProject="src/main/java" />
        <javaClientGenerator targetPackage="tca.gatewayManage.mapper.unifiedService"
                             targetProject="src/main/java" type="ANNOTATEDMAPPER" />
        <!-- 数据库表 --> 
        <!-- <table tableName="create_data_task_tb"><columnOverride column="tdes" jdbcType="VARCHAR" /></table> -->
     
        
    </context>
</generatorConfiguration>